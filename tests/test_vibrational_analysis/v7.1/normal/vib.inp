&FORCE_EVAL
  METHOD Quickstep
  #  STRESS_TENSOR ANALYTICAL
  &DFT
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    POTENTIAL_FILE_NAME GTH_POTENTIALS 
    #SURFACE_DIPOLE_CORRECTION .TRUE.
    #    CHARGE 1
    #    UKS  T
    #    WFN_RESTART_FILE_NAME ./q-28w-RESTART.wfn
    &MGRID
      CUTOFF 600
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-13
      EXTRAPOLATION ASPC 
      EXTRAPOLATION_ORDER 2
    &END QS
    &SCF 
      SCF_GUESS  RESTART 
      EPS_SCF 3.0E-7
      MAX_SCF 500 
      ADDED_MOS 500 
      CHOLESKY INVERSE 
      &SMEAR  ON 
        METHOD FERMI_DIRAC 
        ELECTRONIC_TEMPERATURE [K] 300 
      &END SMEAR 
      &DIAGONALIZATION 
        ALGORITHM STANDARD 
      &END DIAGONALIZATION 
      &MIXING 
        METHOD BROYDEN_MIXING 
        ALPHA   0.20 
        BETA    1.0 
        NBROYDEN  8 
      &END 
      &PRINT 
        &RESTART 
          &EACH 
            QS_SCF 50 
          &END 
          ADD_LAST NUMERIC 
        &END 
      &END 
    &END SCF 

    #    &SCF
    #      SCF_GUESS RESTART 
    #      EPS_SCF 3.0E-7
    #      MAX_SCF 50
    #      &OUTER_SCF
    #         EPS_SCF 3.0E-7
    #         MAX_SCF 5 
    #      &END
    #      &OT
    #         MINIMIZER DIIS 
    #         PRECONDITIONER FULL_SINGLE_INVERSE
    #         ENERGY_GAP 0.1
    #      &END
    #    &END SCF
    &XC
      &XC_FUNCTIONAL PBE 
      &END XC_FUNCTIONAL
      &VDW_POTENTIAL
        DISPERSION_FUNCTIONAL PAIR_POTENTIAL
        &PAIR_POTENTIAL
          TYPE DFTD3
          PARAMETER_FILE_NAME dftd3.dat
          REFERENCE_FUNCTIONAL PBE
        &END PAIR_POTENTIAL
      &END VDW_POTENTIAL
    &END XC
    &PRINT
      &PDOS
        COMPONENTS
        NLUMO -1 
        ADD_LAST  NUMERIC
        &EACH
          MD 50
          GEO_OPT 0
        &END EACH
        COMMON_ITERATION_LEVELS 0
        APPEND T
      &END PDOS

      &V_HARTREE_CUBE ON
        STRIDE 8 8 1
        APPEND T
        &EACH
          MD 50
          GEO_OPT 10
        &END EACH
        ADD_LAST NUMERIC
      &END V_HARTREE_CUBE
      #      &MOMENTS HIGH
      #&END MOMENTS
    &END PRINT
  &END DFT

  &SUBSYS
    &CELL
      ABC [angstrom]  9.006 9.006 27.91875 
      ALPHA_BETA_GAMMA  90 90 90
    &END CELL
    &TOPOLOGY
      COORD_FILE_NAME slab-IrO2-hydrated.xyz
      COORDINATE xyz
    &END TOPOLOGY
    &KIND O
      BASIS_SET DZVP-MOLOPT-SR-GTH 
      POTENTIAL GTH-PBE-q6 
    &END KIND

    &KIND Ir
      BASIS_SET DZVP-MOLOPT-SR-GTH 
      POTENTIAL GTH-PBE-q17
    &END KIND

    &KIND H
      BASIS_SET DZVP-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE-q1
    &END KIND
  &END SUBSYS
&END FORCE_EVAL

&GLOBAL
  PROJECT IrO2
  RUN_TYPE VIBRATIONAL_ANALYSIS
  PRINT_LEVEL MEDIUM
  WALLTIME 23:40:00
&END GLOBAL

&MOTION
  &CONSTRAINT
    &FIXED_ATOMS
      LIST 1 2 3 4 5 6 22 23 24 25 26 27 43 44 45 46 47 48 64 65 66 67 68 69 17 18 20 21 38 39 41 42 59 60 62 63 80 81 83 84 
    &END FIXED_ATOMS
  &END CONSTRAINT

&END MOTION

&VIBRATIONAL_ANALYSIS
  INTENSITIES F
  NPROC_REP 32
  DX 0.001
  FULLY_PERIODIC T
  &PRINT
    &PROGRAM_RUN_INFO ON
    &END
  &END
&END

